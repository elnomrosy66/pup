<form method="POST" action="report/InvForCont">
    <div class="form-group">
        <label for="minDate" class="">من</label>
        <input required type="date" class="form-control datetimepicker2" id="minDate" name="minDate">
        <label for="minDate" class="">إلي</label>
        <input required type="date" class="form-control datetimepicker2" id="maxDate" name="maxDate">
    </div>
    <div class="form-group">
        <label for="minDate" class="">المصدر</label>
        <select required class="form-control" id="source" name="source">
            <option value="داخلى">داخلى</option>
            <option value="خارجي">خارجي</option>
            <option value="مخزون خارجي">مخزون خارجي</option>
            <option value="الكل">الكل</option>
        </select>
    </div>
    <div class="form-group">
        @Html.DropDownList("Cont", ViewBag.Users as IEnumerable<SelectListItem>, "اختر جهة", new { @class = "form-control", required = "required" })
    </div>
    <button type="submit" class="btn btn-success">عرض التقرير</button>

</form>
